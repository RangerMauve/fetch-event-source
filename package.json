  "bugs": {
    "url": "https://github.com/RangerMauve/fetch-event-source/issues"
  },
  "homepage": "https://github.com/RangerMauve/fetch-event-source#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.4",
    "make-fetch": "^2.3.1",
    "rollup": "^2.78.0",
    "tape": "^5.3.1"
  }
