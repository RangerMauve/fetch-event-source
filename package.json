{
  "name": "@rangermauve/fetch-event-source",
  "version": "1.0.3",
  "description": "Implements the EventSource API on top of `fetch()`",
  "type": "module",
  "exports": {
    ".": {
      "import": "./fetch-event-source.js",
      "require": "./index.cjs",
      "default": "./fetch-event-source.js"
    }
  },
  "files": [
    "./fetch-event-source.js",
    "./index.cjs",
